config interface 'lan'
        option ifname 'eth0'
        option proto 'dhcp'

config interface 'N12'
        option proto 'static'
        option netmask '255.255.255.0'
        option delegate '0'
        option ipaddr '10.0.12.2'
        option device 'eth1'

config route
        option target '10.0.0.0'
        option netmask '255.0.0.0'
        option interface 'N12'
        option gateway '10.0.12.1'