config interface 'loopback'
        option device 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'lan'
        option ifname 'eth0'
        option proto 'dhcp'
        list dns '1.1.1.1'

config interface 'N11'
        option proto 'static'
        option netmask '255.255.255.0'
        option delegate '0'
        option ipaddr '10.0.11.1'
        option device 'eth1'

config interface 'N12'
        option proto 'static'
        option netmask '255.255.255.0'
        option delegate '0'
        option ipaddr '10.0.12.1'
        option device 'eth1'

config interface 'N21'
        option proto 'static'
        option netmask '255.255.255.0'
        option delegate '0'
        option ipaddr '10.0.21.1'
        option device 'eth1'

config route
        option interface 'N21'
        option netmask '255.255.255.0'
        option gateway '10.0.21.2'
        option target '10.0.13.0/24'